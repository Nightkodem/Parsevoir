<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Authors>Nightkodem</Authors>
        
        <Company>ThunderFlow</Company>
        <Description>Library for extracting simple data from text using a straightforward interface.</Description>
        <PackageId>Parsevoir</PackageId>
        <Title>Parsevoir</Title>
        <Copyright>GNU General Public License v3.0</Copyright>
        <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/Nightkodem/Parsevoir</RepositoryUrl>
        <PackageTags>Parser</PackageTags>
        <PackageVersion>1.1.3</PackageVersion>
        <RepositoryType>git</RepositoryType>
        <LangVersion>latestmajor</LangVersion>
        
        <NeutralLanguage>en</NeutralLanguage>
        <AssemblyVersion>1.1.3.0</AssemblyVersion>
        
        <SignAssembly>true</SignAssembly>
        <PublicSign>true</PublicSign>
        <AssemblyOriginatorKeyFile>Parsevoir.snk</AssemblyOriginatorKeyFile>
        <TargetFrameworks>net452;net471;net472;net48;net6.0;netcoreapp3.1;netstandard2.0;netstandard2.1</TargetFrameworks>
        <PackageLicenseUrl>https://github.com/Nightkodem/Parsevoir/blob/main/LICENSE</PackageLicenseUrl>
        <PackageReleaseNotes>- Added support for multiple frameworks (.NET standard, .NET framework, .NET Core)
- Added ParseMany functionality</PackageReleaseNotes>
    </PropertyGroup>
    
    <ItemGroup>
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
            <_Parameter1>Parsevoir.Tests, </_Parameter1>
        </AssemblyAttribute>
    </ItemGroup>
    
    <ItemGroup>
      <None Include="..\Parsevoir.snk">
        <Link>ProjectItems\Parsevoir.snk</Link>
      </None>
    </ItemGroup>

</Project>
